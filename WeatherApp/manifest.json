{
  "manifest_version": 3,
  "name": "Bergen Weather Toolbar",
  "version": "1.0",
  "permissions": ["alarms"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Bergen Weather",
    "default_icon": "icons/01d.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "https://api.met.no/*"
  ]
  
  
}